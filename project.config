[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Fake repo created for testing sync between gerrit and github
[access "refs/*"]
	owner = group user/amalx-ssrc
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/amalx-ssrc
	label-Verified = -1..+1 group user/amalx-ssrc
	push = group user/amalx-ssrc
	read = group user/amalx-ssrc
	rebase = group user/amalx-ssrc
	removeReviewer = group user/amalx-ssrc
	submit = group user/amalx-ssrc
[access "refs/heads/*"]
	create = group user/amalx-ssrc
	create = group user/fayadx-ssrc
	push = +force group user/amalx-ssrc
	push = group user/fayadx-ssrc
	pushMerge = group user/amalx-ssrc
	pushMerge = group user/fayadx-ssrc
	read = group user/amalx-ssrc
	read = group user/fayadx-ssrc
	forgeCommitter = group user/fayadx-ssrc
[access "refs/tags/*"]
	push = group user/amalx-ssrc
	push = +force group user/amalx-ssrc
